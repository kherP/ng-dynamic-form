{
  "name": "ng-dynamic-form",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build dynamic-form --prod",
    "test": "ng test dynamic-form --watch=false",
    "lint": "ng lint",
    "pack": "cd dist/dynamic-form && npm pack",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "build-pack-install": "npm run build && npm run pack && npm i dist/dynamic-form/sebgroup-ng-dynamic-form-0.0.1.tgz",
    "storybook": "npm run docs:json && start-storybook -p 6006 --no-manager-cache",
    "build-storybook": "build-storybook -o docs-build -s ./stories/assets",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@angular/animations": "~11.0.5",
    "@angular/common": "~11.0.5",
    "@angular/compiler": "~11.0.5",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~11.0.5",
    "@angular/platform-browser": "~11.0.5",
    "@angular/platform-browser-dynamic": "~11.0.5",
    "@angular/router": "~11.0.5",
    "@sebgroup/ng-components": "^1.0.0-beta.17",
    "@storybook/addon-a11y": "^6.2.9",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.7",
    "@angular/cli": "~11.0.5",
    "@angular/compiler-cli": "~11.0.5",
    "@babel/core": "^7.14.6",
    "@compodoc/compodoc": "^1.1.11",
    "@semantic-release/changelog": "^5.0.1",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/addons": "^6.2.9",
    "@storybook/angular": "^6.2.9",
    "@storybook/theming": "^6.2.9",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "babel-loader": "^8.2.2",
    "codelyzer": "^6.0.0",
    "gh-pages": "^3.2.2",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.0.0",
    "protractor": "~7.0.0",
    "semantic-release": "^17.4.4",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  },
  "release": {
    "pkgRoot": "dist/dynamic-form",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "browserslist": [
    "defaults",
    "IE 11"
  ]
}